  platform :ios, '10.0'
  pod 'Sourcery'
end

target 'Voodoo-tvOS' do
  platform :tvos, '10.0'
  pod 'Sourcery'
end

target 'Voodoo-macOS' do
  platform :macos, '10.14'
  pod 'Sourcery'
end
